<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.islam.malek.E3jazAlQuran">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.islam.malek.E3jazAlQuran.SplashScreen"
            android:theme="@style/splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.islam.malek.E3jazAlQuran.earthActivity" />
        <activity android:name="com.islam.malek.E3jazAlQuran.skyActivity"  />
        <activity android:name="com.islam.malek.E3jazAlQuran.waterActivity"  />
        <activity android:name="com.islam.malek.E3jazAlQuran.doctorActivity" />
        <activity android:name="com.islam.malek.E3jazAlQuran.tashreeActivity"  />
        <activity android:name="com.islam.malek.E3jazAlQuran.MainActivity">
            <intent-filter>
                <action android:name=".ACT_MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.islam.malek.E3jazAlQuran.balaghiActivity"  />
        <activity android:name="com.islam.malek.E3jazAlQuran.createActivity" />
        <activity android:name="com.islam.malek.E3jazAlQuran.foodActivity"  />
        <activity android:name="com.islam.malek.E3jazAlQuran.sonnaActivity"  />
        <activity android:name="com.islam.malek.E3jazAlQuran.quranActvity"></activity>
    </application>

</manifest>